<% if(alert.message != "") { %> 

<div class="alert alert-<%= alert.status %>  alert-dismissible">
    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
    <h5 class="mb-0"><i class="icon fas fa-check"></i> <%= alert.message %> </h5>
</div>

<% } %> 